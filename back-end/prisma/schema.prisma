generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["views"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model person {
  id         String   @db.VarChar(100)
  id_role    Int
  dioseze    String   @db.VarChar(100)
  district   String   @db.VarChar(100)
  church     String   @db.VarChar(100)
  payment    Int?
  year       Int?
  birth_date DateTime @db.Date
  role       role     @relation(fields: [id_role], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@ignore
}

model role {
  id             Int      @id @default(autoincrement())
  role           String   @db.VarChar(100)
  subscription   Int
  year           Int
  age_limitation Int
  person         person[] @ignore
}

model error {
  id    Int       @id @default(autoincrement())
  motif String    @db.VarChar(255)
  date  DateTime? @default(now()) @db.Timestamp(6)
}

/// The underlying view does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
view v_person_details {
  id             String?   @db.VarChar(100)
  id_role        Int?
  role           String?   @db.VarChar(100)
  church         String?   @db.VarChar(100)
  district       String?   @db.VarChar(100)
  dioseze        String?   @db.VarChar(100)
  subscription   Int?
  payment        Int?
  year           Int?
  birth_date     DateTime? @db.Date
  age_limitation Int?
}
